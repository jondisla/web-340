<!-- @format -->
<%- include('header.ejs') %> 
<%- include('navbar.ejs') %>


<div role="main" class="container">
  <br /><br />

  <h2><%= message %></h2>

  <form method="post" action="/process">
    <div class="form-group">
      <input type="text" name="txtName" id="txtName" />
    </div>

    <div class="form-group">
      <!-- CSRF token -->

      <input
        type="hidden"
        name=""
        name="csrf"
        value="<%= csrfToken %>"
      /><button class="btn btn-primary float-right">Submit</button>
    </div>
  </form>
</div>


<%- include ('footer.ejs') %>